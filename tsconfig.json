{
  "compilerOptions": {
    "outDir": "./tsbuild/",
    "sourceMap": false,
    "target": "es2021",
    "module": "commonjs",
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["./test/**/*.ts"]
}
